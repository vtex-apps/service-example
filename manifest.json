{
  "name": "wwg-truefit-prod",
  "vendor": "worldwidegolf",
  "version": "0.0.1",
  "title": "Truefit Integration",
  "description": "Truefit Integration",
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x",
    "node": "6.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x",
    "vtex.product-summary-context": "0.x",
    "vtex.my-account": "1.x",
    "vtex.my-account-commons": "1.x",
    "vtex.apps-graphql": "2.x",
    "vtex.product-context": "0.x"
  },
  "settingsSchema": {
    "title": "Truefit Integration",
    "type": "object",
    "properties": {
      "apiKey": {
        "title": "API Key",
        "description": "Enter the API Key",
        "type": "string"
      },
      "apiToken": {
        "title": "API Token",
        "description": "Enter the API token",
        "type": "string"
      },
      "clientId": {
        "title": "Client Id",
        "description": "Enter the Client Id",
        "type": "string"
      },
      "serverType": {
        "title": "Server Type ",
        "description": "Server Type should be staging or prod",
        "type": "string"
      }
    }
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "worldwidegolf.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
