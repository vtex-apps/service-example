{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "getOrder": {
      "path": "/v0/order/:code",
      "public": true
    },
    "postOrder": {
      "path": "/v0/order/:code/changes",
      "public": true
    },
    "verifySecureAddress": {
      "path": "/v0/verifyaddress/:addressName",
      "public": true
    }
  }
}
